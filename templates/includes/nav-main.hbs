<header class="navbar navbar-default navbar-fixed-top docs-nav" role="banner">
  <div class="container">
    
    <div class="navbar-header">

      <button class="navbar-toggle" type="button" data-toggle="collapse" {{!--data-target=".bs-navbar-collapse"--}} data-target="#bs-example-navbar-collapse-1">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      {{!--
      <a href="{{rel '.'}}/#" class="navbar-brand img-responsive hidden-xs" alt="Forum on Health Care Innovation">
        <img src="{{assets}}/img/logo.png" />
      </a>
      --}}
    </div>

{{!--
    <nav class="collapse navbar-collapse bs-navbar-collapse navbar-right" role="navigation">
      {{> nav-main-links}}
    </nav>
--}}

    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav navbar-right">
        
        {{#is slug 'index'}}
          <li class="dropdown-toggle active">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">About <span class="caret"></span></a>
            <ul class="dropdown-menu" role="menu">
              {{#each ../gsheets.nav}}
                {{#is page 'about'}}
                  {{#is level 2}}
                    <li><a href="#{{navId}}">{{title}}</a></li>
                  {{/is}}
                {{/is}}
              {{/each}}
            </ul>
        </li>
        {{/is}}
        {{#isnt slug 'index'}}
          <li>
            <a href="{{rel '.'}}/#">About</a>
          </li>
        {{/isnt}}
        {{#is slug 'challenge'}}
          <li class="dropdown-toggle active">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Challenge <span class="caret"></span></a>
            <ul class="dropdown-menu" role="menu">
              {{#each ../gsheets.nav}}
                {{#is page 'challenge'}}
                  {{#is level 2}}
                    <li><a href="#{{navId}}">{{title}}</a></li>
                  {{/is}}
                {{/is}}
              {{/each}}
            </ul>
        </li>
        {{/is}}
        {{#isnt slug 'challenge'}}
          <li>
            <a href="{{rel 'challenge'}}/#">Challenge</a>
          </li>
        {{/isnt}}
        {{#is slug 'conference'}}
          <li class="dropdown-toggle active">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Conference <span class="caret"></span></a>
            <ul class="dropdown-menu" role="menu">
              {{#each ../gsheets.nav}}
                {{#is page 'conference'}}
                  {{#is level 2}}
                    <li><a href="#{{navId}}">{{title}}</a></li>
                  {{/is}}
                {{/is}}
              {{/each}}
            </ul>
        </li>
        {{/is}}
        {{#isnt slug 'conference'}}
          <li>
            <a href="{{rel 'conference'}}/#">Conference</a>
          </li>
        {{/isnt}}
        <li class="hidden-xs">
          {{> link-share-twitter}}
        </li>
        <li class="hidden-xs">
          {{> link-share-linkedin}}
        </li>
      </ul>
      
      
    </div><!-- /.navbar-collapse -->

    
    
  </div>
</header>
